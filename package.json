{
  "name": "PinkRides",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios:staging": "react-native run-ios --scheme staging --simulator='iPhone 11'",
    "ios:production": "react-native run-ios --scheme production --simulator='iPhone 11'",
    "ios:release:local": "react-native run-ios --configuration Release",
    "ios:release:staging": "react-native run-ios --scheme staging --configuration Release",
    "ios:release:production": "react-native run-ios --scheme production --configuration Release",
    "android:staging": "set ENVFILE=.env.staging && npx react-native run-android",
    "android:production": "ENVFILE=.env.production react-native run-android",
    "android:apk:local": "cd android && ENVFILE=.env ./gradlew :app:assembleRelease && cd ..",
    "android:apk:staging": "cd android && ENVFILE=.env.staging ./gradlew :app:assembleRelease && cd ..",
    "android:apk:production": "cd android && ENVFILE=.env.production ./gradlew :app:assembleRelease && cd ..",
    "android:bundle": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "pod": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "axios": "^1.7.8",
    "geolib": "^3.3.4",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-device-info": "^14.0.1",
    "react-native-geocoding": "^0.5.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-google-maps": "^1.0.0",
    "react-native-google-places-autocomplete": "^2.5.6",
    "react-native-image-picker": "^7.1.2",
    "react-native-maps": "^1.18.0",
    "react-native-maps-directions": "^1.9.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-otp-entry": "^1.7.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-simple-bottom-sheet": "^1.0.4",
    "react-native-toast-message": "^2.2.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "^13.12.5",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.3.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
